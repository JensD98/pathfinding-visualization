{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Pathfinding",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/Pathfinding/bin/Debug/net5.0/Pathfinding.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Pathfinding",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+https?://\\S+:([0-9]+)",
        "uriFormat": "http://localhost:%s/swagger"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  ]
}
